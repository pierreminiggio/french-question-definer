{
    "name": "pierreminiggio/french-question-definer",
    "description": "Check if it's a french question",
    "type": "library",
    "authors": [
        {
            "name": "Pierre MINIGGIO",
            "email": "pierre@miniggiodev.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PierreMiniggio\\FrenchQuestionDefiner\\": "src/"
